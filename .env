# .env.example
BOT_TOKEN=123456789:AAFxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Recommended: Use paid RPC for trading bot
SOLANA_RPC_HTTPS=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_KEY
SOLANA_RPC_WSS=wss://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_KEY

# Alternative free/public (slow & rate-limited)
# SOLANA_RPC_HTTPS=https://api.mainnet-beta.solana.com

# Jito (optional - for bundles)
JITO_BUNDLE_URL=https://mainnet.block-engine.jito.wtf/api/v1/bundles

# Database / Cache (choose one)
DATABASE_URL=sqlite+aiosqlite:///wtf_bot.db
# or
# REDIS_URL=redis://localhost:6379/0

# Security - used to encrypt private keys in DB
WALLET_ENCRYPTION_KEY=change-this-very-secure-32-byte-key-base64

# Other settings
LOG_LEVEL=INFO
BOT_USERNAME=@WTFTradingBot
SUPPORT_URL=https://t.me/your_support
COMMUNITY_URL=https://t.me/wtf_community
